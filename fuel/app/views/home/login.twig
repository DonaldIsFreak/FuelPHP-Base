{% extends "layout.twig" %}

{% block content %}
<h1>Login</h1>
{% for msg in messages %}
    <div class="alert alert-warning">{{ msg }}</div>
{% endfor %}
<form method="post" action="login">
    User Name:<input type="text" name="username"/><br/>
    Password:<input type="password" name="password"/><br/>
    <button type="submit">Submit</button>
</form>
{% endblock %}